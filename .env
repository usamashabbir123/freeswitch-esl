# FreeSWITCH ESL connection settings
# ESL_HOST can be an IP address or hostname
# For Docker Compose: use 'freeswitch' if on same network
ESL_HOST=192.168.*.*
ESL_PORT=8021
ESL_PASSWORD=Expertflow123

# Logging configuration
LOG_DIR=/var/log/freeswitch-logs
LOG_LEVEL=DEBUG

# Performance tuning
RECONNECT_DELAY=5
FILE_ROTATION_SIZE=104857600
BUFFER_FLUSH_INTERVAL=5
HOST_LOG_PATH=./logs
